class Parent 
{
	public void m1()
	{
		System.out.println("M1-Method");
	}
}
class Child extends Parent 
{
	public void m2()
	{
		System.out.println("M2-Method");
	}
}
class Inheritance_01
{
	public static void main(String[] args)
	{
		Parent p=new Parent();
		p.m1();
		
		Child c=new Child();
		c.m1();
		c.m2();
		
		Parent p1=new Child();
		p1.m1();
		
		//Child c1=new Parent(); // C.T.E  
	}
}
