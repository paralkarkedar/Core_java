/*

Right Side elements
===================
1)
      1 
    2 2 
  3 3 3 
4 4 4 4 

*/


class Example_15 
{
	public static void main(String[] args) 
	{
		for(int i = 1; i <=4; i++){
			
			// space 
			
			for(int j = 4; j>i; j--){
				
				System.out.print("  ");
				
			}
			
			for(int j = 1; j<=i; j++){
			
			System.out.print(i);
			
			}
			System.out.println();
			
		}
	}
}
